{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "lib": ["ES2023", "DOM"],
    "strict": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {},
    "resolveJsonModule": true,
    "rewriteRelativeImportExtensions": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "vitest.config.ts"]
}
